@model List<Category>

<div class="list-group">
    <a href="/movie/list" class="list-group-item list-group-item-light list-group-item-action active">All Categories</a>
    @foreach (var category in Model)
    {
        <a asp-controller="Movie" asp-action="List" asp-route-id="@category.CategoryId" class="list-group-item list-group-item-action">@category.Name</a>
    }
</div>