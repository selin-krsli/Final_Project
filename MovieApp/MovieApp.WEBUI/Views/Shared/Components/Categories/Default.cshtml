@model List<Category>

<div class="list-group">
    <a href="/movies" class="list-group-item list-group-item-light list-group-item-action active">All Categories</a>

    @foreach (var categoryName in Model)
    {
        <a asp-controller="Movie" asp-action="List" asp-route-category="@categoryName.Url" class="list-group-item list-group-item-action">@categoryName.Name</a>
    }
</div>